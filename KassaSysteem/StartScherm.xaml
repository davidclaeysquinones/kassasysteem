<Window x:Class="KassaSysteem.Startscherm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KassaSysteem"
        mc:Ignorable="d"
        Title="Startscherm" Height="700" Width="900">

    <Canvas Name="canvas" >
        
        <Rectangle Width="600" Height="100" Canvas.Bottom="50" Canvas.Left="75" Fill="Lavender">

        </Rectangle>
        <StackPanel Width="900" Height="25" Canvas.Bottom="10">
            <Slider Maximum="100" Height="25" TickPlacement="BottomRight" TickFrequency="5" Name="Slider"></Slider>
        </StackPanel>
        <Button Click="GoToAdmin">Admin</Button>
        
        </Canvas>

</Window>
